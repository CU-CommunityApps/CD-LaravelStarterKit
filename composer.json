{
  "name": "cu-communityapps/cd-laravelstarterkit",
  "description": "Cornell University Custom Development Laravel Starter Kit",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^8.1",
    "livewire/livewire": "^2.10",
    "spatie/laravel-package-tools": "^1.13",
    "cu-communityapps/cwd-framework-lite": "dev-composer"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0",
    "phpunit/phpunit": "^9.5",
    "laravel/pint": "^1.2"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:CU-CommunityApps/cwd_framework_lite.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "CUCustomDev\\LaravelStarterKit\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CUCustomDev\\LaravelStarterKit\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CUCustomDev\\LaravelStarterKit\\StarterKitServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
